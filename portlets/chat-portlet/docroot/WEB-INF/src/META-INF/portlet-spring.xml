<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="com.liferay.chat.service.Chat.base" abstract="true">
		<property name="entryLocalService" ref="com.liferay.chat.service.EntryLocalService.impl" />
		<property name="entryPersistence" ref="com.liferay.chat.service.persistence.EntryPersistence.impl" />
		<property name="entryFinder" ref="com.liferay.chat.service.persistence.EntryFinder.impl" />
		<property name="statusLocalService" ref="com.liferay.chat.service.StatusLocalService.impl" />
		<property name="statusPersistence" ref="com.liferay.chat.service.persistence.StatusPersistence.impl" />
		<property name="statusFinder" ref="com.liferay.chat.service.persistence.StatusFinder.impl" />
	</bean>
	<bean id="com.liferay.chat.service.EntryLocalService.target" class="com.liferay.chat.service.impl.EntryLocalServiceImpl" parent="com.liferay.chat.service.Chat.base">
		<property name="entryLocalService" ref="com.liferay.chat.service.EntryLocalService.target" />
	</bean>
	<bean id="com.liferay.chat.service.EntryLocalService.impl" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean">
		<property name="transactionManager" ref="liferayTransactionManager" />
		<property name="target" ref="com.liferay.chat.service.EntryLocalService.target" />
		<property name="transactionAttributes">
			<props>
				<prop key="add*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="check*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="clear*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="delete*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="set*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="update*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="*">PROPAGATION_SUPPORTS,readOnly</prop>
			</props>
		</property>
	</bean>
	<bean id="com.liferay.chat.service.EntryLocalService.velocity" class="org.springframework.aop.framework.ProxyFactoryBean" parent="baseVelocityService">
		<property name="target" ref="com.liferay.chat.service.EntryLocalService.impl" />
	</bean>
	<bean id="com.liferay.chat.service.EntryLocalServiceUtil" class="com.liferay.chat.service.EntryLocalServiceUtil">
		<property name="service" ref="com.liferay.chat.service.EntryLocalService.impl" />
	</bean>
	<bean id="com.liferay.chat.service.persistence.EntryPersistence.impl" class="com.liferay.chat.service.persistence.EntryPersistenceImpl" parent="basePersistence" />
	<bean id="com.liferay.chat.service.persistence.EntryUtil" class="com.liferay.chat.service.persistence.EntryUtil">
		<property name="persistence" ref="com.liferay.chat.service.persistence.EntryPersistence.impl" />
	</bean>
	<bean id="com.liferay.chat.service.persistence.EntryFinder.impl" class="com.liferay.chat.service.persistence.EntryFinderImpl" parent="basePersistence" />
	<bean id="com.liferay.chat.service.persistence.EntryFinderUtil" class="com.liferay.chat.service.persistence.EntryFinderUtil">
		<property name="finder" ref="com.liferay.chat.service.persistence.EntryFinder.impl" />
	</bean>
	<bean id="com.liferay.chat.service.StatusLocalService.target" class="com.liferay.chat.service.impl.StatusLocalServiceImpl" parent="com.liferay.chat.service.Chat.base">
		<property name="statusLocalService" ref="com.liferay.chat.service.StatusLocalService.target" />
	</bean>
	<bean id="com.liferay.chat.service.StatusLocalService.impl" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean">
		<property name="transactionManager" ref="liferayTransactionManager" />
		<property name="target" ref="com.liferay.chat.service.StatusLocalService.target" />
		<property name="transactionAttributes">
			<props>
				<prop key="add*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="check*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="clear*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="delete*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="set*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="update*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="*">PROPAGATION_SUPPORTS,readOnly</prop>
			</props>
		</property>
	</bean>
	<bean id="com.liferay.chat.service.StatusLocalService.velocity" class="org.springframework.aop.framework.ProxyFactoryBean" parent="baseVelocityService">
		<property name="target" ref="com.liferay.chat.service.StatusLocalService.impl" />
	</bean>
	<bean id="com.liferay.chat.service.StatusLocalServiceUtil" class="com.liferay.chat.service.StatusLocalServiceUtil">
		<property name="service" ref="com.liferay.chat.service.StatusLocalService.impl" />
	</bean>
	<bean id="com.liferay.chat.service.persistence.StatusPersistence.impl" class="com.liferay.chat.service.persistence.StatusPersistenceImpl" parent="basePersistence" />
	<bean id="com.liferay.chat.service.persistence.StatusUtil" class="com.liferay.chat.service.persistence.StatusUtil">
		<property name="persistence" ref="com.liferay.chat.service.persistence.StatusPersistence.impl" />
	</bean>
	<bean id="com.liferay.chat.service.persistence.StatusFinder.impl" class="com.liferay.chat.service.persistence.StatusFinderImpl" parent="basePersistence" />
	<bean id="com.liferay.chat.service.persistence.StatusFinderUtil" class="com.liferay.chat.service.persistence.StatusFinderUtil">
		<property name="finder" ref="com.liferay.chat.service.persistence.StatusFinder.impl" />
	</bean>
</beans>
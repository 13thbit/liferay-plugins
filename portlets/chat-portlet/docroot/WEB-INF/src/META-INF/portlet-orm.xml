<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<entity class="com.liferay.chat.model.impl.EntryImpl" name="Entry">
		<table name="Chat_Entry" />
		<attributes>
			<id name="entryId" />
			<basic name="createDate" />
			<basic name="fromUserId" />
			<basic name="toUserId" />
			<basic name="content" />
		</attributes>
	</entity>
	<entity class="com.liferay.chat.model.impl.StatusImpl" name="Status">
		<table name="Chat_Status" />
		<attributes>
			<id name="statusId" />
			<basic name="userId" />
			<basic name="modifiedDate" />
			<basic name="online">
				<column name="ONLINE_" />
			</basic>
			<basic name="awake" />
			<basic name="activePanelId" />
			<basic name="message" />
			<basic name="playSound" />
		</attributes>
	</entity>
</entity-mappings>
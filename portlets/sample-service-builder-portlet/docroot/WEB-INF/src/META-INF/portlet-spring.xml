<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="com.liferay.sampleservicebuilder.service.FooLocalService.impl" class="com.liferay.sampleservicebuilder.service.impl.FooLocalServiceImpl" lazy-init="true" init-method="init" />
	<bean id="com.liferay.sampleservicebuilder.service.FooLocalService.transaction" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean" lazy-init="true">
		<property name="transactionManager">
			<ref bean="liferayTransactionManager" />
		</property>
		<property name="target">
			<ref bean="com.liferay.sampleservicebuilder.service.FooLocalService.impl" />
		</property>
		<property name="transactionAttributes">
			<props>
				<prop key="add*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="check*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="clear*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="delete*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="set*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="update*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="*">PROPAGATION_SUPPORTS,readOnly</prop>
			</props>
		</property>
	</bean>
	<bean id="com.liferay.sampleservicebuilder.service.FooLocalServiceFactory" class="com.liferay.sampleservicebuilder.service.FooLocalServiceFactory" lazy-init="true">
		<property name="service">
			<ref bean="com.liferay.sampleservicebuilder.service.FooLocalService.transaction" />
		</property>
	</bean>
	<bean id="com.liferay.sampleservicebuilder.service.FooService.impl" class="com.liferay.sampleservicebuilder.service.impl.FooServiceImpl" lazy-init="true" init-method="init" />
	<bean id="com.liferay.sampleservicebuilder.service.FooService.transaction" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean" lazy-init="true">
		<property name="transactionManager">
			<ref bean="liferayTransactionManager" />
		</property>
		<property name="target">
			<ref bean="com.liferay.sampleservicebuilder.service.FooService.impl" />
		</property>
		<property name="transactionAttributes">
			<props>
				<prop key="add*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="check*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="clear*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="delete*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="set*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="update*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="*">PROPAGATION_SUPPORTS,readOnly</prop>
			</props>
		</property>
	</bean>
	<bean id="com.liferay.sampleservicebuilder.service.FooServiceFactory" class="com.liferay.sampleservicebuilder.service.FooServiceFactory" lazy-init="true">
		<property name="service">
			<ref bean="com.liferay.sampleservicebuilder.service.FooService.transaction" />
		</property>
	</bean>
	<bean id="com.liferay.sampleservicebuilder.service.persistence.FooPersistence.impl" class="com.liferay.sampleservicebuilder.service.persistence.FooPersistenceImpl" lazy-init="true" init-method="init">
		<property name="dataSource">
			<ref bean="liferayDataSource" />
		</property>
		<property name="sessionFactory">
			<ref bean="liferaySessionFactory" />
		</property>
	</bean>
	<bean id="com.liferay.sampleservicebuilder.service.persistence.FooUtil" class="com.liferay.sampleservicebuilder.service.persistence.FooUtil" lazy-init="true">
		<property name="persistence">
			<ref bean="com.liferay.sampleservicebuilder.service.persistence.FooPersistence.impl" />
		</property>
	</bean>
</beans>
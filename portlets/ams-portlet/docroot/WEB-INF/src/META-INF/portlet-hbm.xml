<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.ams.model.AMSAsset" />
	<import class="com.liferay.ams.model.AMSCheckout" />
	<import class="com.liferay.ams.model.AMSDefinition" />
	<import class="com.liferay.ams.model.AMSType" />
	<class name="com.liferay.ams.model.impl.AMSAssetImpl" table="AMS_AMSAsset">
		<cache usage="read-write" />
		<id name="amsAssetId" type="long">
			<generator class="assigned" />
		</id>
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="assetDefinitionId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="serialNumber" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="inactiveDate" type="org.hibernate.type.TimestampType" />
		<property name="active" column="active_" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
	</class>
	<class name="com.liferay.ams.model.impl.AMSCheckoutImpl" table="AMS_AMSCheckout">
		<cache usage="read-write" />
		<id name="amsCheckoutId" type="long">
			<generator class="assigned" />
		</id>
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="amsAssetId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="checkOutDate" type="org.hibernate.type.TimestampType" />
		<property name="expectedCheckInDate" type="org.hibernate.type.TimestampType" />
		<property name="actualCheckInDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.liferay.ams.model.impl.AMSDefinitionImpl" table="AMS_AMSDefinition">
		<cache usage="read-write" />
		<id name="assetDefinitionId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="amsTypeId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="manufacturer" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="model" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="orderDate" type="org.hibernate.type.TimestampType" />
		<property name="quantity" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="price" type="com.liferay.portal.dao.orm.hibernate.DoubleType" />
	</class>
	<class name="com.liferay.ams.model.impl.AMSTypeImpl" table="AMS_AMSType">
		<cache usage="read-write" />
		<id name="amsTypeId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>
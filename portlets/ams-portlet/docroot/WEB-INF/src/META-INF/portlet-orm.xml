<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.ams.model.impl.AMSAssetModelImpl">
		<attributes>
			<id name="amsAssetId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="assetDefinitionId" />
			<basic name="serialNumber" />
			<basic name="inactiveDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="active">
				<column name="ACTIVE_" />
			</basic>
			<transient name="primaryKey" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.ams.model.impl.AMSCheckoutModelImpl">
		<attributes>
			<id name="amsCheckoutId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="amsAssetId" />
			<basic name="checkOutDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="expectedCheckInDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="actualCheckInDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<transient name="primaryKey" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.ams.model.impl.AMSDefinitionModelImpl">
		<attributes>
			<id name="assetDefinitionId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="amsTypeId" />
			<basic name="manufacturer" />
			<basic name="model" />
			<basic name="orderDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="quantity" />
			<basic name="price" />
			<transient name="primaryKey" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.ams.model.impl.AMSTypeModelImpl">
		<attributes>
			<id name="amsTypeId" />
			<basic name="groupId" />
			<basic name="name" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.ams.model.impl.AMSAssetImpl" name="AMSAsset">
		<table name="AMS_AMSAsset" />
		<attributes />
	</entity>
	<entity class="com.liferay.ams.model.impl.AMSCheckoutImpl" name="AMSCheckout">
		<table name="AMS_AMSCheckout" />
		<attributes />
	</entity>
	<entity class="com.liferay.ams.model.impl.AMSDefinitionImpl" name="AMSDefinition">
		<table name="AMS_AMSDefinition" />
		<attributes />
	</entity>
	<entity class="com.liferay.ams.model.impl.AMSTypeImpl" name="AMSType">
		<table name="AMS_AMSType" />
		<attributes />
	</entity>
</entity-mappings>
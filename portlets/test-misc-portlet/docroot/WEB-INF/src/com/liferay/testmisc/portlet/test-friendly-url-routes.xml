<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">

<routes>
	<route>
		<pattern>/logo.png</pattern>
		<default-parameter name="p_p_lifecycle">2</default-parameter>
		<default-parameter name="p_p_resource_id">logo.png</default-parameter>
		<ignored-parameter name="p_p_cacheability" />
	</route>
	<route>
		<pattern>{jspPage}</pattern>
		<ignored-parameter name="p_p_cacheability" />
		<ignored-parameter name="p_p_resource_id" />
	</route>
</routes>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="com.liferay.twitter.service.FeedLocalService.impl" class="com.liferay.twitter.service.impl.FeedLocalServiceImpl" />
	<bean id="com.liferay.twitter.service.FeedLocalService.velocity" class="org.springframework.aop.framework.ProxyFactoryBean" parent="baseVelocityService">
		<property name="target" ref="com.liferay.twitter.service.FeedLocalService.impl" />
	</bean>
	<bean id="com.liferay.twitter.service.FeedLocalServiceUtil" class="com.liferay.twitter.service.FeedLocalServiceUtil">
		<property name="service" ref="com.liferay.twitter.service.FeedLocalService.impl" />
	</bean>
	<bean id="com.liferay.twitter.service.persistence.FeedPersistence.impl" class="com.liferay.twitter.service.persistence.FeedPersistenceImpl" parent="basePersistence" />
	<bean id="com.liferay.twitter.service.persistence.FeedUtil" class="com.liferay.twitter.service.persistence.FeedUtil">
		<property name="persistence" ref="com.liferay.twitter.service.persistence.FeedPersistence.impl" />
	</bean>
</beans>
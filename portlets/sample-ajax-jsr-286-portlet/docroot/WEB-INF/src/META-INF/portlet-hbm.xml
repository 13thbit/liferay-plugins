<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="com.liferay.sample.model.impl.NotepadEntryImpl" table="Sample_NotepadEntry">
		<cache usage="read-write" />
		<id name="notepadEntryId" type="long">
			<generator class="assigned" />
		</id>
		<property name="categoryId" type="com.liferay.util.dao.hibernate.LongType" />
		<property name="createDate" />
		<property name="notes" type="com.liferay.util.dao.hibernate.StringType" />
	</class>
</hibernate-mapping>
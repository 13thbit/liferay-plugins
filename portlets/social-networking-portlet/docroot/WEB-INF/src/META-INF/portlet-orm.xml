<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<entity class="com.liferay.socialnetworking.model.impl.MeetupsEntryImpl" name="MeetupsEntry">
		<table name="SN_MeetupsEntry" />
		<attributes>
			<id name="meetupsEntryId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="title" />
			<basic name="description" />
			<basic name="startDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="endDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="totalAttendees" />
			<basic name="maxAttendees" />
			<basic name="price" />
			<basic name="thumbnailId" />
		</attributes>
	</entity>
	<entity class="com.liferay.socialnetworking.model.impl.MeetupsRegistrationImpl" name="MeetupsRegistration">
		<table name="SN_MeetupsRegistration" />
		<attributes>
			<id name="meetupsRegistrationId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="meetupsEntryId" />
			<basic name="status" />
			<basic name="comments" />
		</attributes>
	</entity>
	<entity class="com.liferay.socialnetworking.model.impl.WallEntryImpl" name="WallEntry">
		<table name="SN_WallEntry" />
		<attributes>
			<id name="wallEntryId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="comments" />
		</attributes>
	</entity>
</entity-mappings>
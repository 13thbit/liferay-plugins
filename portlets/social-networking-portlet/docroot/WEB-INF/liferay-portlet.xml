<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>1</portlet-name>
		<header-portlet-css>/summary/css.jsp</header-portlet-css>
		<css-class-wrapper>social-networking-portlet-summary</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>2</portlet-name>
		<icon>/icons/friends.png</icon>
		<social-activity-interpreter-class>com.liferay.socialnetworking.friends.social.FriendsActivityInterpreter</social-activity-interpreter-class>
		<social-request-interpreter-class>com.liferay.socialnetworking.friends.social.FriendsRequestInterpreter</social-request-interpreter-class>
		<header-portlet-css>/friends/css.jsp</header-portlet-css>
		<css-class-wrapper>social-networking-portlet-friends</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>3</portlet-name>
		<icon>/icons/wall.png</icon>
		<friendly-url-mapper-class>com.liferay.socialnetworking.wall.portlet.WallFriendlyURLMapper</friendly-url-mapper-class>
		<social-activity-interpreter-class>com.liferay.socialnetworking.wall.social.WallActivityInterpreter</social-activity-interpreter-class>
		<css-class-wrapper>social-networking-portlet-wall</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>4</portlet-name>
		<friendly-url-mapper-class>com.liferay.socialnetworking.friendsactivities.portlet.FriendsActivitiesFriendlyURLMapper</friendly-url-mapper-class>
		<css-class-wrapper>social-networking-portlet-friends-activities</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>5</portlet-name>
		<icon>/icons/members.png</icon>
		<social-activity-interpreter-class>com.liferay.socialnetworking.members.social.MembersActivityInterpreter</social-activity-interpreter-class>
		<social-request-interpreter-class>com.liferay.socialnetworking.members.social.MembersRequestInterpreter</social-request-interpreter-class>
		<header-portlet-css>/members/css.jsp</header-portlet-css>
		<css-class-wrapper>social-networking-portlet-members</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>6</portlet-name>
		<friendly-url-mapper-class>com.liferay.socialnetworking.map.portlet.MapFriendlyURLMapper</friendly-url-mapper-class>
		<ajaxable>false</ajaxable>
		<css-class-wrapper>social-networking-portlet-map</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>7</portlet-name>
		<friendly-url-mapper-class>com.liferay.socialnetworking.meetups.portlet.MeetupsFriendlyURLMapper</friendly-url-mapper-class>
		<header-portlet-css>/meetups/css.jsp</header-portlet-css>
		<css-class-wrapper>social-networking-portlet-meetups</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
	</portlet>
	<portlet>
		<portlet-name>8</portlet-name>
		<friendly-url-mapper-class>com.liferay.socialnetworking.groupusersactivities.portlet.GroupUsersActivitiesFriendlyURLMapper</friendly-url-mapper-class>
		<css-class-wrapper>social-networking-portlet-group-users-activities</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>
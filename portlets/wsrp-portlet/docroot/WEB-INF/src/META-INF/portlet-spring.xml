<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="com.liferay.wsrp.consumer.service.ProducerLocalService.impl" class="com.liferay.wsrp.consumer.service.impl.ProducerLocalServiceImpl" lazy-init="true" init-method="init" />
	<bean id="com.liferay.wsrp.consumer.service.ProducerLocalService.transaction" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean" lazy-init="true">
		<property name="transactionManager">
			<ref bean="liferayTransactionManager" />
		</property>
		<property name="target">
			<ref bean="com.liferay.wsrp.consumer.service.ProducerLocalService.impl" />
		</property>
		<property name="transactionAttributes">
			<props>
				<prop key="add*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="check*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="clear*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="delete*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="set*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="update*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="*">PROPAGATION_SUPPORTS,readOnly</prop>
			</props>
		</property>
	</bean>
	<bean id="com.liferay.wsrp.consumer.service.ProducerLocalServiceFactory" class="com.liferay.wsrp.consumer.service.ProducerLocalServiceFactory" lazy-init="true">
		<property name="service">
			<ref bean="com.liferay.wsrp.consumer.service.ProducerLocalService.transaction" />
		</property>
	</bean>
	<bean id="com.liferay.wsrp.consumer.service.ProducerService.impl" class="com.liferay.wsrp.consumer.service.impl.ProducerServiceImpl" lazy-init="true" init-method="init" />
	<bean id="com.liferay.wsrp.consumer.service.ProducerService.transaction" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean" lazy-init="true">
		<property name="transactionManager">
			<ref bean="liferayTransactionManager" />
		</property>
		<property name="target">
			<ref bean="com.liferay.wsrp.consumer.service.ProducerService.impl" />
		</property>
		<property name="transactionAttributes">
			<props>
				<prop key="add*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="check*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="clear*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="delete*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="set*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="update*">PROPAGATION_REQUIRED,-com.liferay.portal.PortalException,-com.liferay.portal.SystemException</prop>
				<prop key="*">PROPAGATION_SUPPORTS,readOnly</prop>
			</props>
		</property>
	</bean>
	<bean id="com.liferay.wsrp.consumer.service.ProducerServiceFactory" class="com.liferay.wsrp.consumer.service.ProducerServiceFactory" lazy-init="true">
		<property name="service">
			<ref bean="com.liferay.wsrp.consumer.service.ProducerService.transaction" />
		</property>
	</bean>
	<bean id="com.liferay.wsrp.consumer.service.persistence.ProducerPersistence.impl" class="com.liferay.wsrp.consumer.service.persistence.ProducerPersistenceImpl" lazy-init="true" init-method="init">
		<property name="dataSource">
			<ref bean="liferayDataSource" />
		</property>
		<property name="sessionFactory">
			<ref bean="liferaySessionFactory" />
		</property>
	</bean>
	<bean id="com.liferay.wsrp.consumer.service.persistence.ProducerUtil" class="com.liferay.wsrp.consumer.service.persistence.ProducerUtil" lazy-init="true">
		<property name="persistence">
			<ref bean="com.liferay.wsrp.consumer.service.persistence.ProducerPersistence.impl" />
		</property>
	</bean>
</beans>
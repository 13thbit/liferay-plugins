<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<entity class="com.liferay.wsrp.model.impl.WSRPConsumerImpl" name="WSRPConsumer">
		<table name="WSRP_WSRPConsumer" />
		<attributes>
			<id name="wsrpConsumerId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="name" />
			<basic name="url" />
			<basic name="wsdl" />
		</attributes>
	</entity>
	<entity class="com.liferay.wsrp.model.impl.WSRPConsumerPortletImpl" name="WSRPConsumerPortlet">
		<table name="WSRP_WSRPConsumerPortlet" />
		<attributes>
			<id name="wsrpConsumerPortletId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="wsrpConsumerId" />
			<basic name="name" />
			<basic name="portletHandle" />
		</attributes>
	</entity>
</entity-mappings>
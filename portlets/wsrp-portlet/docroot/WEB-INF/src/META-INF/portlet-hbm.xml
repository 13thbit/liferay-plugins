<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="com.liferay.wsrp.consumer.model.impl.ProducerImpl" table="WSRP_Producer">
		<cache usage="read-write" />
		<id name="producerId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" column="uuid_" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" />
		<property name="modifiedDate" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="wsdlURL" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="markupEndpoint" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="portletManagementEndpoint" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="registrationEndpoint" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="serviceDescriptionEndpoint" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="registrationContext" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="serviceDescription" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>
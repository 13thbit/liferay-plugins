<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_5_1_0.dtd">

<service-builder package-path="com.liferay.wsrp.consumer">
	<namespace>WSRP</namespace>
	<entity name="Producer" uuid="true" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="producerId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="name" type="String" />
		<column name="wsdlURL" type="String" />
		<column name="markupEndpoint" type="String" />
		<column name="portletManagementEndpoint" type="String" />
		<column name="registrationEndpoint" type="String" />
		<column name="serviceDescriptionEndpoint" type="String" />
		<column name="registrationContext" type="String" />
		<column name="serviceDescription" type="String" />

		<!-- Order -->

		<order by="asc">
			<order-column name="name" case-sensitive="false" />
		</order>

		<!-- References -->

		<reference package-path="com.liferay.counter" entity="Counter" />
		<reference package-path="com.liferay.portal" entity="User" />
	</entity>
	<exceptions>
		<exception>InvalidWSDL</exception>
		<exception>Name</exception>
		<exception>WSDLURL</exception>
	</exceptions>

</service-builder>
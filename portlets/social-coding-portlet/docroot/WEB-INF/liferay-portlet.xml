<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 5.3.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_5_3_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>1</portlet-name>
		<icon>/icons/svn.png</icon>
		<scheduler-class>com.liferay.socialcoding.svn.job.SVNScheduler</scheduler-class>
		<friendly-url-mapper-class>com.liferay.socialcoding.svn.portlet.SVNFriendlyURLMapper</friendly-url-mapper-class>
		<social-activity-interpreter-class>com.liferay.socialcoding.svn.social.SVNActivityInterpreter</social-activity-interpreter-class>
		<header-portlet-css>/svn/css.jsp</header-portlet-css>
		<css-class-wrapper>social-coding-portlet-svn</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>2</portlet-name>
		<icon>/icons/jira.png</icon>
		<scheduler-class>com.liferay.socialcoding.jira.job.JIRAScheduler</scheduler-class>
		<social-activity-interpreter-class>com.liferay.socialcoding.jira.social.JIRAActivityInterpreter</social-activity-interpreter-class>
		<header-portlet-css>/jira/css.jsp</header-portlet-css>
		<css-class-wrapper>social-coding-portlet-jira</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>
<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">

<routes>
	<route>
		<pattern>/rss/user/portal/trunk</pattern>
		<default-parameter name="p_p_state">exclusive</default-parameter>
		<default-parameter name="url">/portal/trunk</default-parameter>
	</route>
	<route>
		<pattern>/rss/all/portal/trunk</pattern>
		<default-parameter name="all">1</default-parameter>
		<default-parameter name="p_p_state">exclusive</default-parameter>
		<default-parameter name="url">/portal/trunk</default-parameter>
	</route>
	<route>
		<pattern>/rss/user/plugin/trunk</pattern>
		<default-parameter name="p_p_state">exclusive</default-parameter>
		<default-parameter name="url">/plugin/trunk</default-parameter>
	</route>
	<route>
		<pattern>/rss/all/plugin/trunk</pattern>
		<default-parameter name="all">1</default-parameter>
		<default-parameter name="p_p_state">exclusive</default-parameter>
		<default-parameter name="url">/plugin/trunk</default-parameter>
	</route>
</routes>
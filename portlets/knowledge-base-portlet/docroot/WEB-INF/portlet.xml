<?xml version="1.0"?>

<portlet-app
	version="2.0"
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
>
	<portlet>
		<portlet-name>1</portlet-name>
		<display-name>Knowledge Base</display-name>
		<portlet-class>com.liferay.knowledgebase.admin.portlet.AdminPortlet</portlet-class>
		<init-param>
			<name>config-jsp</name>
			<value>/admin/configuration.jsp</value>
		</init-param>
		<init-param>
			<name>jsp-path</name>
			<value>/admin/</value>
		</init-param>
		<init-param>
			<name>view-jsp</name>
			<value>/admin/view.jsp</value>
		</init-param>
		<init-param>
			<name>copy-request-parameters</name>
			<value>true</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Knowledge Base</title>
			<short-title>Knowledge Base</short-title>
			<keywords>Knowledge Base</keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>articlesOrderByCol</name>
				<value>priority</value>
			</preference>
			<preference>
				<name>articlesOrderByType</name>
				<value>desc</value>
			</preference>
			<preference>
				<name>enableArticleDescription</name>
				<value>false</value>
			</preference>
			<preference>
				<name>enableArticleAssetCategories</name>
				<value>true</value>
			</preference>
			<preference>
				<name>enableArticleAssetTags</name>
				<value>true</value>
			</preference>
			<preference>
				<name>enableArticleRatings</name>
				<value>false</value>
			</preference>
			<preference>
				<name>enableArticleComments</name>
				<value>true</value>
			</preference>
			<preference>
				<name>showArticleComments</name>
				<value>true</value>
			</preference>
			<preference>
				<name>enableArticleViewCountIncrement</name>
				<value>true</value>
			</preference>
			<preference>
				<name>enableTemplateDescription</name>
				<value>false</value>
			</preference>
			<preference>
				<name>enableTemplateComments</name>
				<value>true</value>
			</preference>
			<preference>
				<name>showTemplateComments</name>
				<value>true</value>
			</preference>
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>2</portlet-name>
		<display-name>Knowledge Base Aggregator</display-name>
		<portlet-class>com.liferay.knowledgebase.aggregator.portlet.AggregatorPortlet</portlet-class>
		<init-param>
			<name>config-jsp</name>
			<value>/aggregator/configuration.jsp</value>
		</init-param>
		<init-param>
			<name>jsp-path</name>
			<value>/aggregator/</value>
		</init-param>
		<init-param>
			<name>view-jsp</name>
			<value>/aggregator/view.jsp</value>
		</init-param>
		<init-param>
			<name>copy-request-parameters</name>
			<value>true</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Knowledge Base Aggregator</title>
			<short-title>Knowledge Base Aggregator</short-title>
			<keywords>Knowledge Base Aggregator</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-public-render-parameter>categoryId</supported-public-render-parameter>
		<supported-public-render-parameter>tag</supported-public-render-parameter>
	</portlet>
	<portlet>
		<portlet-name>3</portlet-name>
		<display-name>Knowledge Base Display</display-name>
		<portlet-class>com.liferay.knowledgebase.display.portlet.DisplayPortlet</portlet-class>
		<init-param>
			<name>config-jsp</name>
			<value>/display/configuration.jsp</value>
		</init-param>
		<init-param>
			<name>jsp-path</name>
			<value>/display/</value>
		</init-param>
		<init-param>
			<name>view-jsp</name>
			<value>/display/view.jsp</value>
		</init-param>
		<init-param>
			<name>copy-request-parameters</name>
			<value>true</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Knowledge Base Display</title>
			<short-title>Knowledge Base Display</short-title>
			<keywords>Knowledge Base Display</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-public-render-parameter>categoryId</supported-public-render-parameter>
		<supported-public-render-parameter>tag</supported-public-render-parameter>
	</portlet>
	<portlet>
		<portlet-name>4</portlet-name>
		<display-name>Knowledge Base Search</display-name>
		<portlet-class>com.liferay.knowledgebase.search.portlet.SearchPortlet</portlet-class>
		<init-param>
			<name>config-jsp</name>
			<value>/search/configuration.jsp</value>
		</init-param>
		<init-param>
			<name>jsp-path</name>
			<value>/search/</value>
		</init-param>
		<init-param>
			<name>view-jsp</name>
			<value>/search/view.jsp</value>
		</init-param>
		<init-param>
			<name>copy-request-parameters</name>
			<value>true</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Knowledge Base Search</title>
			<short-title>Knowledge Base Search</short-title>
			<keywords>Knowledge Base Search</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-public-render-parameter>categoryId</supported-public-render-parameter>
		<supported-public-render-parameter>tag</supported-public-render-parameter>
	</portlet>
	<portlet>
		<portlet-name>5</portlet-name>
		<display-name>Knowledge Base List</display-name>
		<portlet-class>com.liferay.knowledgebase.list.portlet.ListPortlet</portlet-class>
		<init-param>
			<name>config-jsp</name>
			<value>/list/configuration.jsp</value>
		</init-param>
		<init-param>
			<name>jsp-path</name>
			<value>/list/</value>
		</init-param>
		<init-param>
			<name>view-jsp</name>
			<value>/list/view.jsp</value>
		</init-param>
		<init-param>
			<name>copy-request-parameters</name>
			<value>true</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Knowledge Base List</title>
			<short-title>Knowledge Base List</short-title>
			<keywords>Knowledge Base List</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-public-render-parameter>categoryId</supported-public-render-parameter>
		<supported-public-render-parameter>tag</supported-public-render-parameter>
	</portlet>
	<public-render-parameter>
		<identifier>categoryId</identifier>
		<qname xmlns:x="http://www.liferay.com/public-render-parameters">x:categoryId</qname>
	</public-render-parameter>
	<public-render-parameter>
		<identifier>tag</identifier>
		<qname xmlns:x="http://www.liferay.com/public-render-parameters">x:tag</qname>
	</public-render-parameter>
</portlet-app>
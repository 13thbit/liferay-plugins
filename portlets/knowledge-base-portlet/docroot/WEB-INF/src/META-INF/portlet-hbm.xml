<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="com.liferay.knowledgebase.model.impl.KBArticleImpl" table="KB_KBArticle">
		<cache usage="read-write" />
		<id name="articleId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" column="uuid_" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="resourcePrimKey" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="title" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="version" type="com.liferay.portal.dao.orm.hibernate.DoubleType" />
		<property name="minorEdit" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="content" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="head" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="template" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="draft" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
	</class>
	<class name="com.liferay.knowledgebase.model.impl.KBArticleResourceImpl" table="KB_KBArticleResource">
		<cache usage="read-write" />
		<id name="resourcePrimKey" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="title" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.liferay.knowledgebase.model.impl.KBFeedbackEntryImpl" table="KB_KBFeedbackEntry">
		<cache usage="read-write" />
		<id name="feedbackEntryId" type="long">
			<generator class="assigned" />
		</id>
		<property name="articleResourcePrimKey" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="comments" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="score" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="vote" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.liferay.knowledgebase.model.impl.KBFeedbackStatsImpl" table="KB_KBFeedbackStats">
		<cache usage="read-write" />
		<id name="feedbackStatsId" type="long">
			<generator class="assigned" />
		</id>
		<property name="articleResourcePrimKey" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="averageScore" type="com.liferay.portal.dao.orm.hibernate.DoubleType" />
		<property name="totalScoreEntries" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="totalVotes" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="yesVotes" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
</hibernate-mapping>
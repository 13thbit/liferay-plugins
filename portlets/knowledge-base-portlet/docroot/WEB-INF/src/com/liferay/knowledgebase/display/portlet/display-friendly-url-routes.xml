<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">

<routes>
	<route>
		<pattern>/{instanceId}/article/{resourcePrimKey:\d+}</pattern>
		<default-parameter name="jspPage">/display/view_article.jsp</default-parameter>
	</route>
	<route>
		<pattern>/{instanceId}/article/{resourcePrimKey:\d+}/history</pattern>
		<default-parameter name="jspPage">/display/history.jsp</default-parameter>
	</route>
	<route>
		<pattern>/{instanceId}/rss</pattern>
		<default-parameter name="p_p_cacheability">cacheLevelPage</default-parameter>
		<default-parameter name="p_p_lifecycle">2</default-parameter>
		<default-parameter name="p_p_resource_id">rss</default-parameter>
		<ignored-parameter name="p_p_state" />
		<ignored-parameter name="jspPage" />
	</route>
</routes>
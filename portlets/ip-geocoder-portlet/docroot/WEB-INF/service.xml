<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_5_0_0.dtd">

<service-builder package-path="com.liferay.ipgeocoder">
	<namespace>IPGeocoder</namespace>
	<entity name="IPInfo" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="ipInfoId" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />

		<!-- Other fields -->

		<column name="ipAddress" type="String" />
		<column name="latitude" type="double" />
		<column name="longitude" type="double" />
		<column name="city" type="String" />
		<column name="country" type="String" />

		<!-- Finder methods -->

		<finder name="IpAddress" return-type="IPInfo">
			<finder-column name="ipAddress" />
		</finder>
	</entity>
</service-builder>
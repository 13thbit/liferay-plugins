<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="com.liferay.ipgeocoder.model.impl.IPInfoImpl" table="IPGeocoder_IPInfo">
		<cache usage="read-write" />
		<id name="ipInfoId" type="long">
			<generator class="assigned" />
		</id>
		<property name="createDate" />
		<property name="ipAddress" type="com.liferay.util.dao.hibernate.StringType" />
		<property name="latitude" type="com.liferay.util.dao.hibernate.DoubleType" />
		<property name="longitude" type="com.liferay.util.dao.hibernate.DoubleType" />
		<property name="city" type="com.liferay.util.dao.hibernate.StringType" />
		<property name="country" type="com.liferay.util.dao.hibernate.StringType" />
	</class>
</hibernate-mapping>
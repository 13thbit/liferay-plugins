<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">

<routes>
	<route>
		<pattern>/user_profile/{userId:\d+}</pattern>
		<default-parameter name="jspPage">/profiles/user_profile.jsp</default-parameter>
		<default-parameter name="p_p_state">exclusive</default-parameter>
	</route>
	<route>
		<pattern>/{jspPageName}</pattern>
		<default-parameter name="p_p_state">exclusive</default-parameter>
		<generated-parameter name="jspPage">/profiles/{jspPageName}.jsp</generated-parameter>
	</route>
</routes>
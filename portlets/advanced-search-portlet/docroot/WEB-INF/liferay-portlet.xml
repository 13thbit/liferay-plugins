<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>1</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>com.liferay.advancedsearch.action.ConfigurationActionImpl</configuration-action-class>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>advancedsearch</friendly-url-mapping>
		<friendly-url-routes>com/liferay/advancedsearch/portlet/advancedsearch-friendly-url-routes.xml</friendly-url-routes>
		<instanceable>true</instanceable>
		<header-portlet-css>/css/main.jsp</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>advancedsearch-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
</liferay-portlet-app>
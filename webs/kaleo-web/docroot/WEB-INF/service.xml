		</finder>
		<finder name="KNI_N" return-type="KaleoTransition">
			<finder-column name="kaleoNodeId" />
			<finder-column name="name" />
		</finder>
		<finder name="KNI_DT" return-type="KaleoTransition">
			<finder-column name="kaleoNodeId" />
			<finder-column name="defaultTransition" />
		</finder>
	</entity>
</service-builder>
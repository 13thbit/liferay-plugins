<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_5_0_0.dtd">

<service-builder package-path="com.liferay.iweb">
	<namespace>IWEB</namespace>

	<entity name="CallBack" local-service="true" remote-service="false">
		<reference package-path="com.liferay.portlet.blogs" entity="BlogsEntry" />
		<reference package-path="com.liferay.portlet.wiki" entity="WikiNode" />
		<reference package-path="com.liferay.portlet.wiki" entity="WikiPage" />
		<reference package-path="com.liferay.portal" entity="Group" />
		<reference package-path="com.liferay.portal" entity="User" />
	</entity>

	<entity name="CrossContainer" local-service="true" remote-service="false">
	</entity>

	<entity name="InterestGroup" local-service="true" remote-service="false">
		<column name="cid" type="long" primary="true" />
		<column name="type" type="int" />
		<column name="semanticsFile" type="Collection" entity="SemanticsFile" mapping-table="InterestGroups_SemanticsFiles" />
	</entity>

	<entity name="IWeb" local-service="true" remote-service="false">
	</entity>

	<entity name="PostEntry" local-service="true" remote-service="false">
		<column name="uid" type="long" primary="true" />
		<column name="interestGroupId" type="long" />
		<column name="pid" type="long" />
		<column name="type" type="String" />
		<column name="semanticsElement" type="Collection" entity="SemanticsElement" mapping-table="PostEntries_SemanticsElements"/>
		<finder name="Id_Type" return-type="Post">
			<finder-column name="pid" />
			<finder-column name="type" />
		</finder>
		<reference package-path="com.liferay.counter" entity="Counter" />
	</entity>

	<entity name="SemanticsElement" local-service="true" remote-service="false">
		<column name="elementURI" type="String" primary="true" />
		<column name="semanticsURI" type="String" />
		<column name="postEntry" type="Collection" entity="PostEntry" mapping-table="PostEntries_SemanticsElements" />
	</entity>

	<entity name="SemanticsFile" local-service="true" remote-service="false">
		<column name="semanticsName" type="String" />
		<column name="semanticsURI" type="String" primary="true" />
		<column name="createdInInterestGroup" type="long" />
		<column name="interestGroup" type="Collection" entity="InterestGroup" mapping-table="InterestGroups_SemanticsFiles" />
		<finder name="CreatedInInterestGroup" return-type="Collection">
			<finder-column name="createdInInterestGroup" />
		</finder>
	</entity>

	<exceptions>
		<exception>IWeb</exception>
	</exceptions>

</service-builder>
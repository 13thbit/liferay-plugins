<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="com.liferay.iweb.model.impl.CommunityImpl" table="IWEB_Community">
		<cache usage="read-write" />
		<id name="cid" type="long">
			<generator class="assigned" />
		</id>
		<property name="type" column="type_" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.liferay.iweb.model.impl.PostImpl" table="IWEB_Post">
		<cache usage="read-write" />
		<id name="uid" type="long">
			<generator class="assigned" />
		</id>
		<property name="pid" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="type" column="type_" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="communityid" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.liferay.iweb.model.impl.SemanticsImpl" table="IWEB_Semantics">
		<cache usage="read-write" />
		<id name="semanticsURI" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="semanticsName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createdInCommunity" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.liferay.iweb.model.impl.SemanticsElementImpl" table="IWEB_SemanticsElement">
		<cache usage="read-write" />
		<id name="elementURI" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="semanticsURI" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>
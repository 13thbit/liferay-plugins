<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" default-init-method="afterPropertiesSet" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean id="com.liferay.saw.service.saw.base" abstract="true">
		<property name="SAWWorkflowLocalService" ref="com.liferay.saw.service.SAWWorkflowLocalService.impl" />
	</bean>
	<bean id="com.liferay.saw.service.SAWWorkflowLocalService.impl" class="com.liferay.saw.service.impl.SAWWorkflowLocalServiceImpl" parent="com.liferay.saw.service.saw.base"></bean>
	<bean id="com.liferay.saw.service.SAWWorkflowLocalService.velocity" class="org.springframework.aop.framework.ProxyFactoryBean" parent="baseVelocityService">
		<property name="target" ref="com.liferay.saw.service.SAWWorkflowLocalService.impl" />
	</bean>
	<bean id="com.liferay.saw.service.SAWWorkflowLocalServiceUtil" class="com.liferay.saw.service.SAWWorkflowLocalServiceUtil">
		<property name="service" ref="com.liferay.saw.service.SAWWorkflowLocalService.impl" />
	</bean>
</beans>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="com.liferay.ruon.model.impl.PresenceUserImpl" table="Ruon_PresenceUser">
		<cache usage="read-write" />
		<id name="presenceUserId" type="long">
			<generator class="assigned" />
		</id>
		<property name="presenceStatus" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.liferay.ruon.model.impl.PresenceStatusesImpl" table="Ruon_PresenceStatuses">
		<cache usage="read-write" />
		<id name="presenceStatusId" type="long">
			<generator class="assigned" />
		</id>
		<property name="presenceStatusMessage" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>
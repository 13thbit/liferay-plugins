<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 5.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_5_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>5.2.1+</version>
	</compatibility>
	<theme id="my-office" name="My Office">
		<color-scheme id="01" name="Clean">
			<default-cs>true</default-cs>
			<css-class>clean</css-class>
		</color-scheme>
		<color-scheme id="02" name="Cluttered">
			<css-class>cluttered</css-class>
		</color-scheme>
		<color-scheme id="03" name="Dirty">
			<css-class>dirty</css-class>
		</color-scheme>
	</theme>
</look-and-feel>
<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 5.3.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_5_3_0.dtd">

<look-and-feel>
	<compatibility>
		<version>5.2.3+</version>
	</compatibility>
	<theme id="highlighter" name="Highlighter" >
		<color-scheme id="01" name="Hot Pink">
			<default-cs>true</default-cs>
			<css-class>pink</css-class>
		</color-scheme>
		<color-scheme id="02" name="Nuclear Blue">
			<css-class>blue</css-class>
		</color-scheme>
		<color-scheme id="03" name="Neon Green">
			<css-class>green</css-class>
		</color-scheme>
		<color-scheme id="04" name="Pencil Yellow">
			<css-class>yellow</css-class>
		</color-scheme>
	</theme>
</look-and-feel>
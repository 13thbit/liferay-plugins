<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 5.1.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_5_1_0.dtd">

<look-and-feel>
	<compatibility>
		<version>5.1.0+</version>
	</compatibility>
	<theme id="desktop" name="Desktop">
		<settings>
			<setting key="css-template-id" value="CSS-TEMPLATE" />
		</settings>
		<layout-templates>
			<custom>
				<layout-template id="freeform" name="Desktop">
					<template-path>/layouttpl/custom/freeform.tpl</template-path>
					<wap-template-path>/layouttpl/custom/freeform.wap.tpl</wap-template-path>
					<thumbnail-path>/layouttpl/custom/freeform.png</thumbnail-path>
				</layout-template>
			</custom>
		</layout-templates>
	</theme>
</look-and-feel>
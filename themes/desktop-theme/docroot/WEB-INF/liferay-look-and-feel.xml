<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 5.3.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_5_3_0.dtd">

<look-and-feel>
	<compatibility>
		<version>5.3.0+</version>
	</compatibility>
	<theme id="desktop" name="Desktop">
		<root-path>/</root-path>
		<templates-path>${root-path}/templates</templates-path>
		<images-path>${root-path}/images</images-path>
		<template-extension>vm</template-extension>
		<color-scheme id="01" name="Liferay">
			<default-cs>true</default-cs>
			<css-class></css-class>
			<color-scheme-images-path>${images-path}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="02" name="Mac">
			<css-class>osx</css-class>
			<color-scheme-images-path>${images-path}/color_schemes/osx</color-scheme-images-path>
		</color-scheme>
	</theme>
</look-and-feel>
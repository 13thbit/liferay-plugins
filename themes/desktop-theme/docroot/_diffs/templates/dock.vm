<div id="dock-wrapper">
	<div id="taskbar">
		<div class="lfr-dock interactive-mode">

			<ul class="lfr-dock-list">
				#if ($show_home)
					<li class="home">
						<a href="$home_url">$home_text</a>

						#if ($has_navigation && ($css_class.contains("vista")))
							#parse ("$full_templates_path/navigation.vm")
						#end
					</li>
				#end

				#if ($show_my_places && ($css_class.contains("vista")))
					<li class="my-places">
						<a href="javascript: ;">$my_places_text</a>

						$theme.myPlaces()
					</li>
				#end

				#if ($show_my_account)
					<li class="my-account">
						<a href="$my_account_url">$my_account_text</a>
					</li>
				#end

				#if ($show_add_content)
					<li class="add-content">
						<a href="javascript: void(0);" onclick="LiferayDesktop.addContentAnimate()">$add_content_text</a>
					</li>
					<li class="layout">
						<a href="javascript: void(0);" onclick="$layout_url">$layout_text</a>
					</li>
				#end

				#if ($show_page_settings)
					<li class="page-settings">
						<a href="$page_settings_url">$page_settings_text</a>
					</li>
				#end

				#if ($show_staging)
					<li class="staging">
						<a>$staging_text</a>

						$theme.staging()
					</li>
				#end

				#if ($show_sign_in)
					<li class="sign-in">
						<a href="$sign_in_url">$sign_in_text</a>
					</li>
				#end

				#if ($show_sign_out)
					<li class="sign-out">
						<a href="$sign_out_url">$sign_out_text</a>
					</li>
				#end
			</ul>

			#if ($has_navigation && ($css_class.contains("vista")))
				<div class="vista-menu">
					#parse ("$full_templates_path/navigation.vm")
				</div>
			#end
		</div>

		<div id="taskbar-portlets-wrapper">
			<ul id="taskbar-portlets"></ul>
		</div>
	</div>
</div>
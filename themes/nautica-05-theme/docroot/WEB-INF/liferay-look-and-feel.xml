<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 5.1.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_5_1_0.dtd">

<look-and-feel>
	<compatibility>
		<version>5.1.0+</version>
	</compatibility>
	<theme id="nautica-05" name="Nautica">
		<root-path>/</root-path>
		<templates-path>${root-path}/templates</templates-path>
		<images-path>${root-path}/images</images-path>
		<color-scheme id="01" name="Light">
			<css-class>light</css-class>
			<default-cs>true</default-cs>
			<color-scheme-images-path>${images-path}/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="02" name="Dark">
			<css-class>dark</css-class>
		</color-scheme>
	</theme>
</look-and-feel>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>5.2.1+</version>
	</compatibility>
	<theme id="pure" name="Pure">
		<color-scheme id="01" name="Blue - dark">
			<default-cs>true</default-cs>
			<css-class>blue-dark</css-class>
		</color-scheme>
		<color-scheme id="02" name="Blue - light">
			<css-class>blue-light</css-class>
		</color-scheme>
		<color-scheme id="03" name="Gray">
			<css-class>gray</css-class>
		</color-scheme>
		<color-scheme id="04" name="Green">
			<css-class>green</css-class>
		</color-scheme>
		<color-scheme id="05" name="Orange">
			<css-class>orange</css-class>
		</color-scheme>
		<color-scheme id="06" name="Pink">
			<css-class>pink</css-class>
		</color-scheme>
		<color-scheme id="07" name="Purple">
			<css-class>purple</css-class>
		</color-scheme>
		<color-scheme id="08" name="Red">
			<css-class>red</css-class>
		</color-scheme>
		<color-scheme id="09" name="Tan">
			<css-class>tan</css-class>
		</color-scheme>
		<color-scheme id="10" name="White">
			<css-class>white</css-class>
		</color-scheme>
	</theme>
</look-and-feel>
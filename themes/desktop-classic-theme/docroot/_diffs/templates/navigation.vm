<div id="navigation-container">
	<ul class="menu">
		#if ($has_nav_item)
			<li class="selected"><a href="$selected_item.getURL()"><span>$selected_item.getName()</span></a></li>
		#else
			<li class="selected"><a href="$sign_in_url"><span>$sign_in_text</span></a></li>
		#end

		<li class="spacer"></li>

		#foreach ($nav_item in $nav_items)
			#if (!$nav_item.isSelected())
				<li class="item"><a href="$nav_item.getURL()"><span>$nav_item.getName()</span></a></li>
			#end
		#end
	</ul>

	#parse ("$full_templates_path/dock.vm")
</div>
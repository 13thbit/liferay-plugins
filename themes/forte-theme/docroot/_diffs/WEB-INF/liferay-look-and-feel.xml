<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 5.1.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_5_1_0.dtd">

<look-and-feel>
	<compatibility>
		<version>5.1.0+</version>
	</compatibility>
	<theme id="forte" name="Forte">
		<root-path>/</root-path>
		<templates-path>${root-path}/templates</templates-path>
		<images-path>${root-path}/images</images-path>
		<color-scheme id="01" name="blue">
			<css-class>blue</css-class>
			<color-scheme-images-path>${images-path}/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="02" name="yellow">
			<css-class>yellow</css-class>
		</color-scheme>
		<color-scheme id="03" name="green">
			<css-class>green</css-class>
		</color-scheme>
		<color-scheme id="04" name="red">
			<css-class>red</css-class>
		</color-scheme>
	</theme>
</look-and-feel>

<div id="navigation">
	<ul>
		#foreach ($nav_item in $nav_items)
			#if ($nav_item.isSelected())
				<li class="selected">
					<a href="$nav_item.getURL()">$nav_item.getName()</a>
				</li>
			#else
				<li>
					<a href="$nav_item.getURL()">$nav_item.getName()</a>
				</li>
			#end
		#end
	</ul>
</div>